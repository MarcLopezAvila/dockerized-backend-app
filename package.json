{
  "name": "dockerized-backend-app",
  "version": "0.0.0",
  "private": true,
  "main": "build/app.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "dev-docker": "docker-compose up --build --abort-on-container-exit server",
    "prod": "tsc && node ./build/app.js"
  },
  "dependencies": {
    "color-log": "^0.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "morgan": "~1.9.1",
    "pg": "^8.8.0",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "concurrently": "^7.5.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  }
}
